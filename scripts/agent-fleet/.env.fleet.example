# Agent Fleet (stress testing) â€” separate from main app .env
# Copy to .env.fleet and fill in. The fleet loads only this file, not .env.

# Azure OpenAI (required for fleet)
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_BASE_PATH=https://your-resource.azure-api.net/openai/deployments
AZURE_OPENAI_DEPLOYMENT=gpt-4o-mini-2024-07-18
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# API to hit (local for stress testing, or production)
CLAWTRADE_API_URL=http://localhost:3001

# Fleet behavior (fleet + simulator both use this file; target server = CLAWTRADE_API_URL above)
NUM_AGENTS=5
FLEET_WAKE_INTERVAL_MS=900000
FLEET_VERBOSE=1
# Simulator only: trades per agent (default 10)
# TRADES_PER_AGENT=10
